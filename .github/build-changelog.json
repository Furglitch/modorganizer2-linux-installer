{
    "categories": [
        {
            "title": "## üöÄ Features",
            "labels": ["enhancement"]
        },
        {
            "title": "## üêõ Bugfixes",
            "labels": ["bug"]
        },
        {
            "title": "## üì• Updates",
            "labels": ["update"]
        }
    ],
    "ignore": [
        "ignore"
    ],
    "sort": {
        "order": "ASC",
        "on_property": "mergedAt"
    },
    "template": "${{CHANGELOG}}\n\n---\n\n**Full Changelog**: #{{RELEASE_DIFF}}",
    "pr_template": "- ${{TITLE}} - PR #${{NUMBER}} - ${{AUTHOR}}",
    "empty_template": "No changes were made in this release.",
    "max_pull_requests": 1000,
    "max_back_track_time_days": 1000,
    "exclude_merge_branches": [
        "prerelease"
    ],
    "base_branches": [
        "master"
    ]
}